 [   131,   1+y,    64,    31,    -31,     -8]
 [   196,   1+y,    64,    31,    -31,    -38]
 [   261,   1+y,    64,    31,    -31,     -8]
 [   326,   1+y,    64,    31,    -31,    -38]
 [   391,   1+y,    64,    39,    -31,     -8]
 [   456,   1+y,    64,    39,    -31,    -38]
}

if (param_tunnel == 0) {
  if (climate == CLIMATE_TROPIC) {
  	replace replace_tunnels_subtropic (2389, "gfx/TUNNELS.png") {
  		template_road_tunnels(120)
  	}
  	replace replace_tunnels_desert (2421, "gfx/TUNNELS.png") {
  		template_road_tunnels(160)
  	}
  }	else
  if (climate == CLIMATE_ARCTIC) {
  	replace replace_tunnels_subarctic (2389, "gfx/TUNNELS.png") {
  		template_road_tunnels(40)
  	}
  	replace replace_tunnels_arctic (2421, "gfx/TUNNELS.png") {
  		template_road_tunnels(80)
  	}
  }	else	{
  	replace replace_tunnels_temperate (2389, "gfx/TUNNELS.png") {
  		template_road_tunnels(0)
  	}
  }
}
